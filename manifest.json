{

  "manifest_version": 2,
  "name": "sCustom",
  "version": "1.0",
  "description": "Ini Adalah addons Firefox untuk memudahkan",
  "homepage_url": "https://github.com/usermh99/sCustom",
  "icons": {
    "48": "icon/TAG.png"
  },
  "content_scripts": [
    {
      "matches": ["https://simpeg.kemenkumham.go.id/devp/siap/skp_journal.php"],
      "js": [
	  "j/jquery-1.8.3.js",
	  "j/jquery-ui.min.js",
	  ]

    }
  ],
    "permissions": [
    "<all_urls>", "storage"]
    ,
	 "browser_action": {
    "default_icon": "icon/TAG.png",
    "default_title": "sCustom-Memudahkan",
     },
	"background": {
    "scripts": ["j/sCustom.js"]
  }

}
